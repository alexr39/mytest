name LOCAL_HSI_SERVER
tagged front-port 1/1
exit
vlan 2149
name FOR_STB
tagged port-channel 1
exit
ip igmp snooping enable
ip igmp proxy report enable
ip igmp proxy report range 224.0.0.1 239.255.255.255 from all to 30


